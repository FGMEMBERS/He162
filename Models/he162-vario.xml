<?xml version="1.0"?>
<PropertyList>

 <path>he162-vario.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>minute</object-name>
  <property>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
  <interpolation>
   <entry>
    <ind>-10000</ind>
    <dep>-170</dep>
   </entry>
   <entry>
    <ind>-5905.5</ind>
    <dep>-135</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>5905.5</ind>
    <dep>135</dep>
   </entry>
   <entry>
    <ind>10000</ind>
    <dep>170</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


</PropertyList>
